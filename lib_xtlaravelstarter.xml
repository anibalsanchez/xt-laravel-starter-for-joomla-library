  <description>
    <![CDATA[
XT Laravel Starter for Joomla. This project is a good starting point to begin using the Laravel Framework on Joomla.
  ]]>
  </description>
	<media destination="lib_xtlaravelstarter" folder="media">
		<folder>public</folder>
	</media>
  <files>
    <filename>lib_xtlaravelstarter.xml</filename>

    <filename>lib_xtlaravelstarter.xml</filename>
    <filename>package-lock.json</filename>
    <filename>package.json</filename>
    <filename>tailwind.config.js</filename>
    <filename>webpack.mix.js</filename>

    <folder>app</folder>
    <folder>bootstrap</folder>
    <folder>config</folder>
    <folder>database</folder>
    <folder>resources</folder>
    <folder>routes</folder>
    <folder>storage</folder>
    <folder>vendor</folder>
    <folder>vendor_prefixed</folder>
  </files>
